{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*.js"],
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js",
      "headers": {
        "Cache-Control": "no-store"
      }
    }
  ],
  "middleware": [
    {
      "src": "/(.*)",
      "middlewarePath": "middleware.js"
    }
  ]
}
